{
  "extends": ["next/core-web-vitals", "next/typescript", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "@typescript-eslint/ban-ts-comment": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "semi": ["error", "always"],
    "eol-last": ["error", "always"],
    "prettier/prettier": ["error", { "semi": true, "endOfLine": "lf" }]
  }
}
